<p id="notice"><%= notice %></p>
<button class="btn btn-large btn-success btn-block"><i class="icon-th-list icon-white"></i> Detail Jurnal</button>
<table class="table table-bordered">
<tr>
  <td><strong>Issn:</strong></td>
  <td><%= @journal.issn %></td>
</tr>
  <td><strong>Volume:</strong></td>
  <td><%= @journal.volume %></td>
</tr>
  <td><strong>Title:</strong></td>
  <td><%= @journal.title %></td>
</tr>
  <td><strong>Authors:</strong></td>
  <td><%= raw @journal.authors %></td>
</tr>
  <td><strong>Abstract:</strong></td>
  <td><%= raw @journal.abstract %></td>
</tr>
  <td><strong>Date:</strong></td>
  <td><%= @journal.date %></td>
</tr>
</table>

